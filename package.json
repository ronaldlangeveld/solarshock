{
    "name": "solarshock",
    "version": "0.0.0",
    "author": "Ronald Langeveld",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "dev": "tsc --watch --preserveWatchOutput --sourceMap",
        "test:watch": "jest --watchAll --collectCoverage",
        "build": "tsc",
        "prepare": "tsc",
        "test": "jest",
        "test:types": "tsc --noEmit",
        "lint:code": "eslint src/ --ext .ts --cache",
        "lint": "yarn lint:code && yarn lint:test",
        "lint:test": "eslint -c test/.eslintrc.js test/ --ext .ts --cache"
    },
    "files": [
        "build"
    ],
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.1.5",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "@typescript-eslint/parser": "^5.59.2",
        "c8": "^7.14.0",
        "concurrently": "^8.0.1",
        "eslint": "^8.40.0",
        "eslint-plugin-ghost": "^2.17.0",
        "jest": "^29.5.0",
        "mocha": "^10.2.0",
        "sinon": "^15.0.4",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    },
    "c8": {
        "exclude": [
            "src/**/*.d.ts",
            "test/**/*.ts"
        ]
    },
    "dependencies": {
        "bcrypt": "^5.1.0"
    }
}
